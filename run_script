#!/bin/bash

#450 drives, sequence 32, 64, 128
python3 main.py -nd 450 -ndtrain 100 -seq 64 -nnsize 32 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_32_450 -mso 0 -lff 0
python3 main.py -nd 450 -ndtrain 100 -seq 64 -nnsize 64 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_64_450 -mso 0 -lff 0
python3 main.py -nd 450 -ndtrain 100 -seq 64 -nnsize 128 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_128_450 -mso 0 -lff 0
#350 drives, sequence 32, 64, 128
python3 main.py -nd 350 -ndtrain 100 -seq 64 -nnsize 32 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_32_350 -mso 0 -lff 0
python3 main.py -nd 350 -ndtrain 100 -seq 64 -nnsize 64 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_64_350 -mso 0 -lff 0
python3 main.py -nd 350 -ndtrain 100 -seq 64 -nnsize 128 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_128_350 -mso 0 -lff 0
#250 drives, sequence 32, 64, 128
python3 main.py -nd 250 -ndtrain 100 -seq 64 -nnsize 32 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_32_250 -mso 0 -lff 0
python3 main.py -nd 250 -ndtrain 100 -seq 64 -nnsize 64 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_64_250 -mso 0 -lff 0
python3 main.py -nd 250 -ndtrain 100 -seq 64 -nnsize 128 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_128_250 -mso 0 -lff 0
#150 drives, sequence 32, 64, 1280
python3 main.py -nd 150 -ndtrain 100 -seq 64 -nnsize 32 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_32_150 -mso 0 -lff 0
python3 main.py -nd 150 -ndtrain 100 -seq 64 -nnsize 64 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_64_150 -mso 0 -lff 0
python3 main.py -nd 150 -ndtrain 100 -seq 64 -nnsize 128 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_128_150 -mso 0 -lff 0
#50 drives, sequence 32, 64, 128
python3 main.py -nd 50 -ndtrain 100 -seq 64 -nnsize 32 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_32_50 -mso 0 -lff 0
python3 main.py -nd 50 -ndtrain 100 -seq 64 -nnsize 64 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_64_50 -mso 0 -lff 0
python3 main.py -nd 50 -ndtrain 100 -seq 64 -nnsize 128 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_128_50 -mso 0 -lff 0
#20 drives, sequence 32, 64, 128
python3 main.py -nd 20 -ndtrain 100 -seq 64 -nnsize 32 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_32_20 -mso 0 -lff 0
python3 main.py -nd 20 -ndtrain 100 -seq 64 -nnsize 64 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_64_20 -mso 0 -lff 0
python3 main.py -nd 20 -ndtrain 100 -seq 64 -nnsize 128 -nnlayers 2 -balanced 1 -bs 256 -tt 0 -name seq_128_20 -mso 0 -lff 0